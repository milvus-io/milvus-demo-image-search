{
  "name": "milvus",
  "version": "0.1.0",
  "private": true,
  "description": "milvus",
  "dependencies": {
    "antd": "^3.25.3",
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^0.9.1",
    "electron-installer-dmg": "^3.0.0",
    "foreman": "^3.0.1",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0"
  },
  "devDependencies": {
    "electron": "^7.1.7",
    "electron-installer-debian": "^2.0.1",
    "electron-packager": "^14.1.1"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "electron": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "package-mac": "electron-packager ./ milvus --overwrite --platform=darwin --arch=x64  --out=electron-app",
    "package-linux": "electron-packager ./ milvus --overwrite --platform=linux --arch=x64  --out=electron-app",
    "deb64-linux": "electron-installer-debian --src electron-app/milvus-linux-x64/ --dest electron-app/installers/ --arch amd64",
    "mac-dmg": "electron-installer-dmg ./milvus-darwin-x64/milvus.app milvus --out=electron-app/installers/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}